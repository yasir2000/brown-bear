<section class="empty-state-page">
    <div class="empty-state-illustration">
        {{> svg-permission-denied }}
    </div>

    <h1 class="empty-state-title">
        {{# gettext }}You have a restricted user account{{/ gettext }}
    </h1>
    <p class="empty-state-text">
        {{# gettext }}You cannot access to this page.{{/ gettext }}
    </p>

    <a class="empty-state-action tlp-button-primary" href="/my/">
        <i class="fa fa-reply tlp-button-icon"></i> {{# gettext }}Go to my home page{{/ gettext }}
    </a>
</section>
