<section class="tlp-pane">
    <div class="tlp-pane-container">
        <div class="tlp-pane-header">
            <h1 class="tlp-pane-title">SOAP API</h1>
        </div>
        <section class="tlp-pane-section">
            <div class="alert tlp-alert-warning">Those APIs are deprecated. Whenever possible
                there shouldn't be any new developments done relying on them.</div>
            <ul>
            {{# end_points }}
                <li>{{ title }} - v{{ version }}
                    <p>{{{ description }}}</p>
                    <ul>
                        <li><a href="{{ changelog }}">ChangeLog</a></li>
                        <li><a href="{{ wsdl_viewer }}">WSDL Documentation</a></li>
                        <li><a href="{{ wsdl }}">WSDL</a></li>
                    </ul>
                </li>
            {{/ end_points }}
            </ul>
        </section>
    </div>
</section>
