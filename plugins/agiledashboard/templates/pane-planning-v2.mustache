<div class="tlp-framed-horizontally"
    ng-app="planning"
    ng-controller="MainController"
>
    <div class="planning-init-data" ng-non-bindable
         data-user-id="{{ user_id }}"
         data-project-id="{{ project_id }}"
         data-milestone-id="{{ milestone_id }}"
         data-language="{{ lang }}"
         data-view-mode="{{ view_mode }}"
         data-user-accessibility-mode="{{ user_accessibility_mode }}"
         data-is-in-explicit-top-backlog="{{ is_in_explicit_top_backlog }}"
         data-allowed-additional-panes-to-display="{{ allowed_additional_panes_to_display }}"
         data-is-list-picker-enabled="{{ is_list_picker_enabled }}"
         data-is-links-field-v2-enabled="{{ is_links_field_v2_enabled }}"
         data-trackers-ids-having-list-picker-disabled="{{ trackers_ids_having_list_picker_disabled }}"
         data-has-current-project-parents="{{ has_current_project_parents }}"
    ></div>
    <planner-app class="planning"></planner-app>
</div>
