{{# milestone_presenter }}
    <h1 class="testplan-root-title">
        <div class="tlp-dropdown">
            <div id="agiledashboard-scrum-milestone-menu">
                {{ milestoneTitle }}
                <i class="fa fa-fw fa-caret-down" id="agiledashboard-scrum-milestone-menu-caret"></i>
            </div>
            <div class="tlp-dropdown-menu" role="menu">
                <a href="{{ editArtifactUrl }}"
                   class="backlog-item-link tlp-dropdown-menu-item"
                   data-artifact-id="{{ milestoneId }}"
                   role="menuitem"
                >
                    {{ editArtifact }}
                </a>
            </div>
        </div>
        {{# displayMilestoneDates }}
            <small>
                {{startDate}} â€“ {{endDate}}
            </small>
        {{/ displayMilestoneDates }}
    </h1>

    <div class="main-project-tabs">
        <nav class="tlp-tabs">
            {{# getPaneInfoList }}
                <a href="{{ getUri }}" class="tlp-tab {{# isActive }}tlp-tab-active{{/ isActive }}">
                    {{ getTitle }}
                    {{# isExternalLink }}
                        <i class="fas fa-external-link-alt tlp-tab-icon"></i>
                    {{/ isExternalLink }}
                </a>
            {{/ getPaneInfoList }}
            <a href="/plugins/tracker/?aid={{ milestoneId }}" class="tlp-tab">
            {{ artifact }}
            <i class="fas fa-external-link-alt tlp-tab-icon"></i>
            </a>
        </nav>
    </div>
{{/ milestone_presenter }}
