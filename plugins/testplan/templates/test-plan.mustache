{{> milestone-header }}

<div class="testplan-root">
    <div id="test-plan"
         data-user-display-name="{{ user_display_name }}"
         data-project-id="{{ project_id }}"
         data-project-name="{{ project_name }}"
         data-milestone-id="{{ milestone_id }}"
         data-milestone-url="{{ milestone_url }}"
         data-milestone-title="{{ milestone_title }}"
         data-parent-milestone-title="{{ parent_milestone_title }}"
         data-user-can-create-campaign="{{ user_can_create_campaign }}"
         data-test-definition-tracker-id="{{ test_definition_tracker_id }}"
         data-test-definition-tracker-name="{{ test_definition_tracker_name }}"
         data-expand-backlog-item-id="{{ expand_backlog_item_id }}"
         data-highlight-test-definition-id="{{ highlight_test_definition_id }}"
         data-platform-name="{{ platform_name }}"
         data-platform-logo-url="{{ platform_logo_url }}"
         data-base-url="{{ base_url }}"
         data-artifact-links-types="{{ artifact_links_types }}"
    ></div>
</div>
